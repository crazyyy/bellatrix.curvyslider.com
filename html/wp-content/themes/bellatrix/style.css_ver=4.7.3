	background:#787878;
}


/*Posts Navigation*/
.posts-navigation{
	position: relative;
	margin: 0 0 30px;
	display: inline-block;
	width: 100%;
	font-size:16px;
}
.boxed-layout .posts-navigation{
	margin: 3.750em -1.875em;
