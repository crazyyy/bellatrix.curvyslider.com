.post-view{font-size:inherit;}
.post-view-single{line-height:18px; font-size:inherit; float:left;}

.post-ascii-link {
    position: relative;
    display: inline-block;
    width: 18px;
    height: 18px;
    top: 5px;
    border-radius: 100%;
    box-shadow: 0px 0px 0px 2px #787878;
