.rotate-shadow{
	transform:rotate(180deg);
	-ms-transform:rotate(180deg);
	-webkit-transform:rotate(180deg);
	position: absolute;
	bottom: 0px;
	left: 0px;
	height: 10px;
	width: 100%;
	opacity:0.7;
	filter:alpha(opacity=70);
}
