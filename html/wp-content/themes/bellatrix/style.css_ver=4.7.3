/*HR Shadow*/
.hr-shadow-mask{
	height:13px;
	overflow:hidden;	
	opacity:0.5;
	filter: alpha(opacity=50);
}
.hr-shadow-mask.shadow-dark{opacity:1; filter: alpha(opacity=100);}
