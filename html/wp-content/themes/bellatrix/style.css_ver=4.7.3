.post-format-container .icomoon-icon{
	font-size:24px;
	position: absolute;
	left: 50%;
	top: 50%;
	margin-top: -12px;
	margin-left: -12px;
}
.title-container{
	font-size:20px; 
	font-weight:800; 
	display:block; 
	width:100%; 
	margin-bottom:14px; 
